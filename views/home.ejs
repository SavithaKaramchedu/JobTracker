    <script>
        $("#mySearch").on("keyup", function () {
            var value = $(this).val().toLowerCase();
            $("#parentAccountTable tr").filter(function () {
                $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
            });
        });
       
      
    </script>
   
    <script>
        var searchbar = '';

        function searchbarchange(moduleClicked) {
            searchbar = moduleClicked;

            $('#_globalSearch').val('');
          
        }
        $(document).ready(function () {

            $('#Salesdropdown').click(function () {
                $('#Editopp').css('display', 'none');
                $('#btn-sales').css('display', 'block');
