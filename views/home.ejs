    <style>
        .modal-footer {

            border-radius: 0 0 3px 3px;
            background: #ecf0f1;

        }

        /* width */
        ::-webkit-scrollbar {
            width: 8px;
            height: 8px;
        }

        #main-container {
            padding: 10px !important;
        }

        /* Track */
        ::-webkit-scrollbar-track {
            box-shadow: inset 0 0 5px #337ab7;
            border-radius: 10px;
            width: 8px;

        }

        /* Handle */
        ::-webkit-scrollbar-thumb {
            background: #337ab7;
            border-radius: 10px;
            width: 8px;

        }

        /* Handle on hover */
        ::-webkit-scrollbar-thumb:hover {
            background: #3382b7;
            width: 8px;

        }

        ::placeholder {

            font-size: 12px;
        }

        input[type=text] {
            width: 100%;
            height: 50%;
            border-radius: 0px;
        }

        textarea.form-control {
            height: 38px;
            width: 173%;
        }

        .form-control {

            height: calc(1.00em + .75rem + 2px);
        }

        .form-control1 {

            height: calc(1.00em + .75rem + 2px);
        }

        .btn-secondary {
            color: #fff;
            background-color: rgb(206, 212, 218);
            border-color: #adb5bd;
        }

        .modal.fade.in {
            /* top: 300px; */
        }

        .modal.fade.in {
            top: 99px;
        }

        #line {
            background-color: lightgrey;
            height: 17px;
            border-radius: 2px;
            width: 90%;
            float: left;
            position: relative;
            left: 32px;
        }

        select {
            font-size: 13px !important;
        }

        .customtr:hover {
            background-color: lightgrey
        }

